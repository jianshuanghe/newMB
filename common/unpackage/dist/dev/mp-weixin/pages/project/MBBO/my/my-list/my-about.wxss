
.my-about{
	width: 100%;
	height: 242rpx;
	background: #FFFFFF;
	margin-top: 20rpx;
}
.my-about-company{
	margin: 0 auto;
	width: 90%;
	height: 120rpx;
	border-bottom: 2rpx solid #F5F5F5;
}
.my-about-company>view:nth-of-type(1){
	font-size: 28rpx;
	color: #2E2E30;
	line-height: 120rpx;
	float: left;
}
.my-about-company>view:nth-of-type(2){
	width: 10%;
	height: 100%;
	float: right;
	line-height: 120rpx;
	text-align: center;
}
.my-about-company>view:nth-of-type(2)>image{
	width: 18rpx;
	height: 18rpx;
}
.my-about-company:nth-of-type(2){
	padding-bottom:300rpx;
}

