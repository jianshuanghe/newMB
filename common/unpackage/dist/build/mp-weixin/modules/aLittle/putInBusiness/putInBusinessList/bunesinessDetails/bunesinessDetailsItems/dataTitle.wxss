
.resuleTab-content{
	position: fixed;
	width: 750rpx;
	background: #FFFFFF;
	z-index: 101;
}
.resuleTab{
	position: relative;
	width: 100%;
	margin-bottom: 10rpx;
}
.resuleTab-box{
	position: relative;
	width: 70%;
	margin-top: 10rpx;
	margin: auto;
}
.RT-box{
	position: relative;
	width: 50%;
}
.active-tap{
	font-size: 34rpx !important;
	color: #2E2E30 !important;
	font-weight: bold;
}
.RT-box-top{
	font-family: PingFang-SC-Bold;
	font-size: 32rpx;
	color: #5D5D5D;
	letter-spacing: 0;
	text-align: center;
	line-height: 40rpx;
	margin: 6rpx 0;
}
.RT-box-bot{
	margin-top: 10rpx;
	background: #02C2A2;
	border-radius: 100px;
	position: relative;
	width: 40rpx;
	height: 4rpx;
	margin: auto;
}

