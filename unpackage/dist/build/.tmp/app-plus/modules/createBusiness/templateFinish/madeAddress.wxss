
.mySetup-address{
	width: 100vw;
	min-height: 100vh;
	position: fixed;
	top: 0;
	background: #fff;
	z-index: 9999;
	/* border: 1px solid #FFFFFF; */
}
.my-title {
	width: 100%;
	height: 88rpx;
	background: #ffffff;
	border-bottom: 2rpx solid #f5f5f5;
	position: fixed;
	top: 0;
	z-index: 999;
}
.quickBtn-Fd {
	width: 5%;
	margin-left: 38rpx;
}
.search-Fd {
	position: relative;
	width: 100%;
	text-align: center;
	line-height: 88rpx;
	font-size: 28rpx;
	color: #2e2e30;
}
.tiancong{
	width: 100%;
	height: 68rpx;
}
.mySetup-address-con{
	width: 100%;
	min-height: 240rpx;
	background: #FFFFFF;
	margin-top: 20rpx;
}
.mySetup-address-nei{
	width: 90%;
	min-height: 160rpx;
	margin: 0 auto;
	border-bottom: 2rpx solid #D2D2D2;
}
.mySetup-address-nei>view:nth-of-type(1){
	font-size: 32rpx;
	color: #2E2E30;
	padding-top: 30rpx;
}
.mySetup-address-nei>view:nth-of-type(2){
	font-size: 26rpx;
	color: #9B9B9B;
}
.mySetup-address-xuan{
	width: 90%;
	min-height: 70rpx;
	margin: 0 auto;
}
.mySetup-address-xuan-img{
	width: 40%;
	height: 100%;
	font-size: 26rpx;
	color: #2E2E30;
	line-height: 76rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	float: left;
}
.mySetup-address-xuan-img>image{
	width: 32rpx;
	height: 32rpx;
	display: block;
	margin-top: 20rpx;
}
.mySetup-address-xuan-img>._span{
	margin-left: 20rpx;
}
.mySetup-address-xuan-gai{
	width: 25%;
	height: 100%;
	font-size: 26rpx;
	color: #2E2E30;
	float: right;
}
.mySetup-address-xuan-gai>view{
	width: 50%;
	height: 100%;
	text-align: center;
	line-height: 76rpx;
	float: left;
}
.mySetup-address-di{
	width: 100%;
	height: 10rpx;
	line-height: 2rpx;
}
.mySetup-address-di>image{
	width: 100%;
	height: 100%;
}
.mySetup-address-sub{
	width: 100%;
	height: 100rpx;
	background: #02C2A2;
	text-align: center;
	line-height: 100rpx;
	font-size: 32rpx;
	color: #FFFFFF;
	position: fixed;
	bottom: 0;
}
.tiancongs{
	width: 100%;
	height: 120rpx;
}

