	bottom: 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack:center;
	-webkit-justify-content:center;
	        justify-content:center;
}
.QRcode-sub>view{
	width: 16%;
	height: 100%;
