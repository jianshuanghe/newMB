	width: 32rpx;
	height: 32rpx;
	margin-top: 18rpx;
}
.QRcode-sub>view>view:nth-child(2){
	font-size: 24rpx;
	color: #02C2A2;
	padding-top: 10rpx;
}
.beizi{
	color:  #D2D2D2!important;
}
