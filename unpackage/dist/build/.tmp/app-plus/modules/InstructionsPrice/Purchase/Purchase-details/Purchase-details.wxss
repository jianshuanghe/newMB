}
.QRcode-sub>view>view:nth-child(1){
	width: 72rpx;
	height: 72rpx;
	border-radius: 50%;
	background: #F0FCFA;
	margin: 20rpx auto auto auto;
}
.bei{
	background: #F5F5F5!important;
}
