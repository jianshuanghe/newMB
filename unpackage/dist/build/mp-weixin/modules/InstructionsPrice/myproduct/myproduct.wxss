
.myproduct{
	width: 100%;
	min-height: 100%;
	background: #FFFFFF;
}
.tian{
	width: 100%;
	height: 88rpx;
}
.my-title {
	width: 100%;
	height: 88rpx;
	background: #ffffff;
	border-bottom: 2rpx solid #f5f5f5;
	position: fixed;
	top: 0;
	z-index: 999;
}
.quickBtn-Fd {
	width: 5%;
	margin-left: 38rpx;
}
.myproduct-title{
	width: 60%;
	height: 100%;
	margin-left: 120rpx;
	line-height: 90rpx;
	float: left;
}
.myproduct-title>._span{
	font-size: 32rpx;
	color: #9B9B9B;
	margin-left: 40rpx;
}
.changeColora{
	color: #02C2A2!important;
}
.myproduct-titleall{
	width: 20%;
	height: 100%;
	font-size: 28rpx;
	color: #2E2E30;
	float: right;
	line-height: 88rpx;
	text-align: center;
	position: relative;
}
.myproduct-titleall>image{
	width: 14rpx;
	height: 14rpx;
	margin-left: 5rpx;
}
.myproduct-titleall-T{
	width: 300rpx;
	height: 438rpx;
	background: #2E2E30;
	background:rgba(46,46,48,0.95);
	border-radius: 8rpx;
	position: absolute;
	top: 88rpx;
	right: 30rpx;
}
.myproduct-titleall-T>view{
	width: 80%;
	height: 108rpx;
	border-bottom: 2rpx solid  #444444;
	margin: 0 auto;
	text-align: center;
	line-height: 108rpx;
	font-size: 28rpx;
	color: #FFFFFF;
}
.myproduct-titleall-T>view:last-child{
	border: 0;
}
.change{
	color: #02C2A2!important;
}
.myproduct-list{
	width: 90%;
	height: 722rpx;
	margin: 0 auto;
	border-bottom: 2rpx solid #F5F5F5;
}
.myproduct-list-name{
	font-size: 40rpx;
	color: #2E2E30;
	line-height: 64rpx;
	padding-top: 40rpx;
}
.myproduct-list-time{
	font-size: 28rpx;
	color: #9B9B9B;
	padding-top: 14rpx;
}
.myproduct-list-img{
	width: 100%;
	height: 368rpx;
	margin-top: 20rpx;
	position: relative;
}
.myproduct-list-img>image:nth-of-type(1){
	width: 100%;
	height: 100%;
}
.tishis{
	width: 100rpx;
	height: 100rpx;
	position: absolute;
	top: 0;
	right: 0;
}
.myproduct-list-edit{
	width: 100%;
	height: 40rpx;
	margin-top: 30rpx;
}
.mybusiness-supply-bot{
	width: 100%;
	height: 120rpx;
	border-top: 2rpx solid #F5F5F5;
	position: fixed;
	bottom: 0;
	background: #FFFFFF;
}
.mybusiness-supply-Customer{
	width: 95%;
	height:90rpx;
	background: #02C2A2;
	margin: 15rpx auto;
	border-radius: 2px;
	font-size: 32rpx;
	color: #FFFFFF;
	text-align: center;
	line-height: 90rpx;
}
.mybusiness-supply-contact{
	width: 59%;
	height: 100%;
	float: left;
	background-image: -o-linear-gradient(224deg, #15D49F 0%, #57D6CF 100%);
	background-image: linear-gradient(-134deg, #15D49F 0%, #57D6CF 100%);
	font-size: 32rpx;
	color: #FFFFFF;
	text-align: center;
	line-height: 100rpx;
}
.icon-list{
	position: relative;
	width: 100%;
	margin-top: 10rpx;
}
.icon-list>._div:nth-of-type(1){
	margin-left: 0;
}
.icon-list>._div:nth-of-type(6){
	border-right: 0!important;
}
.lists{
	position: relative;
	width: 14%;
	height: 30rpx;
	margin-left: 22rpx;
	border-right: 2rpx solid #E2E2E2;
}
.list-item{
	position: relative;
	width: 100%;
}
.icon-img{
	position: relative;
	width: 30%;
}
.icon-img>._img{
	position: relative;
	width: 4.8vw;
	height: 4.8vw;
}
.icon-text{
	position: relative;
	width: 40%;
}
.icon-text>._p{
	position: relative;
	width: 100%;
	margin-left: 2vw;
	font-size: 3.2vw;
	line-height: 5vw;
	color: #9B9B9B;
}
.interested{
	position: absolute;
	right: 0;
}

