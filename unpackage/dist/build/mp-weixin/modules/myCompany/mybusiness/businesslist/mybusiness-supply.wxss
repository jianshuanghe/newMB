
.mybusiness-supply{
	width: 100%;
	min-height: 50%;
	background: #FFFFFF;
	padding-bottom: 120rpx;
}
.mybusiness-supply-con{
	width: 90%;
	height: 352rpx;
	margin: 0 auto;
	border-bottom: 2rpx solid #F5F5F5;
	overflow: hidden;
}
.mybusiness-supply-conone{
	width: 100%;
	height: 232rpx;
	margin-top: 40rpx;
}
.mybusiness-supply-conone-zi{
	width: 60%;
	height: 100%;
	float: left;
}
.mybusiness-supply-conone-zi>view:nth-of-type(1){
	width: 100%;
	height: 40rpx;
	font-size: 34rpx;
	color: #2E2E30;
	line-height: 40rpx;
	overflow: hidden;/*超出部分隐藏*/
	-o-text-overflow:ellipsis;
	   text-overflow:ellipsis;/* 超出部分显示省略号 */
	white-space: nowrap;/*规定段落中的文本不进行换行 */
}
.mybusiness-supply-conone-zi>view:nth-of-type(2){
	width: 100%;
	height: 40rpx;
	line-height: 40rpx;
}
.mybusiness-supply-conone-zi>view:nth-of-type(2)>._span{
	padding: 4rpx 4rpx 4rpx 4rpx;
	background: #FFF7E5;
	font-size: 20rpx;
	color: #FE9D08;
	margin-left: 10rpx;
}
.mybusiness-supply-conone-zi>view:nth-of-type(3){
	width: 100%;
	height: 40rpx;
	line-height: 40rpx;
	font-size: 22rpx;
	color: #9B9B9B;
	padding-left: 12rpx;
	padding-top: 10rpx;
}
.mybusiness-supply-conone-zi>view:nth-of-type(3)>image{
	width: 20rpx;
	height: 25rpx;
	display: block;
	float: left;
	margin-top: 8rpx;
}
.mybusiness-supply-conone-zi>view:nth-of-type(3)>._span{
	display: block;
	margin-left: 10rpx;
	float: left;
}
.mybusiness-supply-conone-zi>view:nth-of-type(4){
	width: 100%;
	height: 40rpx;
	line-height: 40rpx;
	font-size: 28rpx;
	color: #2E2E30;
	padding-left: 12rpx;
}
.mybusiness-supply-conone-zi>view:nth-of-type(5){
	width: 100%;
	height: 40rpx;
	line-height: 40rpx;
	padding-left: 12rpx;
}
.mybusiness-supply-conone-zi>view:nth-of-type(5)>._span:nth-of-type(1){
	border: 2rpx solid #02C2A2;
	border-radius: 4rpx;
	padding: 6rpx 6rpx 6rpx 6rpx;
	font-size: 20rpx;
	color: #02C2A2;
}
.mybusiness-supply-conone-zi>view:nth-of-type(5)>._span:nth-of-type(2){
	font-size: 22rpx;
	color: #9B9B9B;
	margin-left: 10rpx;
}
.mybusiness-supply-conone-tu{
	width: 226rpx;
	height: 166rpx;
	float: right;
	border-radius: 4rpx;
}
.mybusiness-supply-conone-tu>image{
	width: 100%;
	height: 100%;
}
.mybusiness-supply-contwo{
	width: 100%;
	height: 60rpx;
}
.mybusiness-supply-bot{
	width: 100%;
	height: 120rpx;
	border-top: 2rpx solid #F5F5F5;
	position: fixed;
	bottom: 0;
	background: #FFFFFF;
}
.mybusiness-supply-Customer{
	width: 95%;
	height:90rpx;
	background: #02C2A2;
	margin: 15rpx auto;
	border-radius: 2px;
	font-size: 32rpx;
	color: #FFFFFF;
	text-align: center;
	line-height: 90rpx;
}
.mybusiness-supply-contact{
	width: 59%;
	height: 100%;
	float: left;
	background-image: -o-linear-gradient(224deg, #15D49F 0%, #57D6CF 100%);
	background-image: linear-gradient(-134deg, #15D49F 0%, #57D6CF 100%);
	font-size: 32rpx;
	color: #FFFFFF;
	text-align: center;
	line-height: 100rpx;
}
.icon-list{
	position: relative;
	width: 100%;
	margin-top: 10rpx;
	margin-left: 10rpx;
}
.lists{
	position: relative;
	width: 16.6%;
}
.list-item{
	position: relative;
	width: 100%;
}
.icon-img{
	position: relative;
	width: 30%;
}
.icon-img>._img{
	position: relative;
	width: 4.8vw;
	height: 4.8vw;
}
.icon-text{
	position: relative;
	width: 40%;
}
.icon-text>._p{
	position: relative;
	width: 100%;
	margin-left: 2vw;
	font-size: 3.2vw;
	line-height: 5vw;
	color: #9B9B9B;
}
.interested{
	position: absolute;
	right: 0;
}

