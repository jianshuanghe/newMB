
.mySetup-address-new{
	width: 100%;
	min-height: 100%;
}
.my-title {
	width: 100%;
	height: 88rpx;
	background: #ffffff;
	border-bottom: 2rpx solid #f5f5f5;
	position: fixed;
	top: 0;
	z-index: 999;
}
.quickBtn-Fd {
	width: 5%;
	margin-left: 38rpx;
}
.search-Fd {
	position: relative;
	width: 100%;
	text-align: center;
	line-height: 88rpx;
	font-size: 28rpx;
	color: #2e2e30;
}
.tiancong{
	width: 100%;
	height: 88rpx;
}
.mySetup-address-new-detailed{
	width: 100%;
	height: 570rpx;
	background: #FFFFFF;
}
.mySetup-address-new-name{
	width: 90%;
	height: 120rpx;
	border-bottom: 2rpx solid #D2D2D2;
	margin: 0 auto;
}
.mySetup-address-new-name>view:nth-child(1){
	font-size: 28rpx;
	color: #2E2E30;
	line-height: 120rpx;
	height: 100%;
	width: 20%;
	float: left;
}
.mySetup-address-new-name>view:nth-child(2){
	font-size: 28rpx;
	line-height: 120rpx;
	height: 100%;
	width: 40%;
	float: left;
}
.mySetup-address-new-name>view:nth-child(2)>input{
	width: 100%;
	height: 100%;
}
.mySetup-address-new-name>view:nth-child(3){
	width: 5%;
	height: 100%;
	line-height: 148rpx;
	float:right;
}
.mySetup-address-new-name>view:nth-child(3)>image{
	width: 32rpx;
	height: 40rpx;
}
.ziti{
	margin-top: 36rpx;
	color: #BDBDBD;
}
.zitis{
	margin-top: 36rpx;
	color: black;
}
.mySetup-address-new-names{
	width: 90%;
	height: 120rpx;
	border-bottom: 2rpx solid #D2D2D2;
	margin: 0 auto;
}
.mySetup-address-new-names>view:nth-child(1){
	font-size: 28rpx;
	color: #2E2E30;
	line-height: 120rpx;
	height: 100%;
	width: 20%;
	float: left;
}
.mySetup-address-new-names>view:nth-child(2){
	font-size: 28rpx;
	line-height: 120rpx;
	height: 100%;
	width: 75%;
	float: left;
}
.mySetup-address-new-names>view:nth-child(2)>input{
	width: 100%;
	height: 100%;
}
.mySetup-address-new-names>view:nth-child(3){
	width: 5%;
	height: 100%;
	line-height: 148rpx;
	float:right;
}
.mySetup-address-new-names>view:nth-child(3)>image{
	width: 32rpx;
	height: 40rpx;
}
.mySetup-address-new-mo{
	width: 90%;
	height: 70rpx;
	margin: 0 auto;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	line-height: 76rpx;
	margin-top: 5rpx;
}
.mySetup-address-new-mo>image{
	width: 32rpx;
	height: 32rpx;
	display: block;
	margin-top: 20rpx;
}
.mySetup-address-new-mo>._span{
	margin-left: 20rpx;
}
.mySetup-address-new-sub{
	width: 90%;
	height: 90rpx;
	margin: 0 auto;
	background: #02C2A2;
	text-align: center;
	line-height: 90rpx;
	font-size: 32rpx;
	color: #FFFFFF;
	margin-top: 120rpx;
}
.mySetup-address-img{
	width: 40rpx!important;
	height: 40rpx;
	right: 10rpx!important;
}

