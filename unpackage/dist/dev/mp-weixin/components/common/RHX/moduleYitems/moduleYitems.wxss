
.content{
	position: relative;
	width: 20vw;
}
.moduleYitems-content{
	position: relative;
	width: 150rpx;
}
.moduleYitems{
	position: relative;
	width: 80rpx;
	height: 80rpx;
	margin: auto;
	border-radius: 50%;
	background: #BDBDBD;
}
.items-img{
	pposition: relative;
	width: 40rpx;
	height: 40rpx;
	margin: auto;
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
}
.items-text{
	font-family: PingFangSC-Regular;
	font-size: 24rpx;
	color: #5D5D5D;
	text-align: center;
	margin-top: 16rpx;
	overflow:hidden;
	text-overflow:ellipsis;
	-o-text-overflow:ellipsis;
	-webkit-text-overflow:ellipsis;
	-moz-text-overflow:ellipsis;
	white-space:nowrap;
}
.moduleYitemsAC{
	position: relative;
	width: 80rpx;
	height: 80rpx;
	margin: auto;
	border-radius: 50%;
	background-image: -webkit-linear-gradient(316deg, #15D49F 0%, #57D6CF 100%);
	background-image: linear-gradient(134deg, #15D49F 0%, #57D6CF 100%);
	overflow:hidden;
	text-overflow:ellipsis;
	-o-text-overflow:ellipsis;
	-webkit-text-overflow:ellipsis;
	-moz-text-overflow:ellipsis;
	white-space:nowrap;
}
.items-textAC{
	font-family: PingFangSC-Regular;
	font-size: 24rpx;
	color: #02C2A2;
	text-align: center;
	margin-top: 16rpx;
}

