
.myproduct-list{
	width: 90%;
	height: 660rpx;
	margin: 0 auto;
	border-bottom: 2rpx solid #F5F5F5;
}
.myproduct-list-name{
	font-size: 40rpx;
	color: #2E2E30;
	line-height: 64rpx;
	padding-top: 40rpx;
}
.myproduct-list-time{
	font-size: 28rpx;
	color: #9B9B9B;
	padding-top: 14rpx;
}
.myproduct-list-img{
	width: 100%;
	height: 368rpx;
	margin-top: 20rpx;
	position: relative;
}
.myproduct-list-img>image:nth-of-type(1){
	width: 100%;
	height: 100%;
}
.tishis{
	width: 100rpx;
	height: 100rpx;
	position: absolute;
	top: 0;
	right: 0;
}
.myproduct-list-edit{
	width: 100%;
	height: 40rpx;
	margin-top: 30rpx;
}
.product-book-BA{
	width: 13.5%;
	height: 26rpx;
	margin-top: 20rpx;
	line-height: 26rpx;
	font-size: 24rpx;
	color: #9B9B9B;
	border-right: 2rpx solid #E2E2E2;
	float: left;
	margin-left: 22rpx;
}
.right{
	border: 0;
}
.one{
	margin-left: 0;
}
.shou>image{
	width: 24rpx!important;
	height: 24rpx!important;
	margin-top: 0rpx !important;
}
.product-book-BA>image{
	width: 24rpx;
	height: 20rpx;
	margin-top: 2rpx;
	display: block;
	float: left;
}
.product-book-BA>._span{
	margin-left: 10rpx;
	display: block;
	float: left;
}
.myproduct-titleall{
	width: 20%;
	height: 50rpx;
	font-size: 28rpx;
	color: #2E2E30;
	float: right;
	top: -82rpx;
	line-height: 88rpx;
	text-align: center;
	z-index: 1000;
	position: relative;
	position: fixed;
	top: 0rpx;
	right: 0;
}
.colors{
	color: #02C2A2!important;
}
.myproduct-titleall>image{
	width: 14rpx;
	height: 14rpx;
	margin-left: 5rpx;
}
.myproduct-titleall-T{
	width: 300rpx;
	height: 438rpx;
	background: #2E2E30;
	background:rgba(46,46,48,0.95);
	border-radius: 8rpx;
	position: absolute;
	top: 88rpx;
	right: 30rpx;
}
.myproduct-titleall-T>view{
	width: 80%;
	height: 108rpx;
	border-bottom: 2rpx solid  #444444;
	margin: 0 auto;
	text-align: center;
	line-height: 108rpx;
	font-size: 28rpx;
	color: #FFFFFF;
}
.myproduct-titleall-T>view:last-child{
	border: 0;
}
.change{
	color: #02C2A2!important;
}
.meirenkanwo{
	width: 330rpx;
	height: 280rpx;
	display: block;
	margin: 120rpx auto auto auto;
	font-size: 28rpx;
	text-align: center;
	color: #9B9B9B;
	clear: both;
}
.meirenkanwo>image {
	width: 92%;
	height: 80%;
}

