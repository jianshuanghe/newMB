
.Order-details{
	width: 100%;
	min-height: 100%;
}
.tian{
	width: 100%;
	height: 88rpx;
}
.news-system-title{
	width: 100%;
	height: 88rpx;
	background: #FFFFFF;
	border-bottom: 2rpx solid #F5F5F5;
	position: fixed;
	top: 0;
	z-index: 999;
}
.quickBtn-Fd{
	width: 5%;
	margin-left: 38rpx;
}
.search-Fd{
	position: relative;
	width: 23%;
	margin-left: 38%;
	text-align: center;
	line-height: 88rpx;
	font-size: 28rpx;
	color: #2E2E30;
	float: left;
}
.Order-details-A{
	width: 100%;
	height: 160rpx;
	border-bottom: 2rpx solid #F5F5F5;
	background: #FFFFFF;
}
.Order-details-Aone{
	font-size: 32rpx;
	color: #FAB100;
	padding-top: 40rpx;
	padding-left: 30rpx;
}
.Order-details-Atwo{
	font-size: 28rpx;
	color: #BDBDBD;
	height: 30rpx;
	line-height: 30rpx;
	padding-left: 30rpx;
}
.Order-details-AA{
	width: 100%;
	height: 160rpx;
	position: relative;
	background: #FFFFFF;
}
.Order-details-AA>view:nth-of-type(1){
	font-size: 32rpx;
	color: #2E2E30;
	padding-top: 30rpx;
	padding-left: 30rpx;
}
.Order-details-AA>view:nth-of-type(2){
	font-size: 26rpx;
	color: #9B9B9B;
	padding-left: 30rpx;
}
.Order-details-AA>image{
	width: 100%;
	height: 4rpx;
	position: absolute;
	bottom: 0;
}
.Order-details-B{
	width: 100%;
	min-height: 100rpx;
	border-bottom: 2rpx solid #EFEFEF;
	background: #FFFFFF;
	position: relative;
	margin-top: 20rpx;
}
.Supply-order-tu{
	left: 0;
	top: 0;
	width: 100rpx;
	height: 100rpx;
	position: absolute;
}
.Order-details-B>view:nth-of-type(1){
	font-size: 32rpx;
	color: #2E2E30;
	line-height: 100rpx;
	padding-left: 70rpx;
	float: left;
	font-weight: bold;
}
.Order-details-D{
	width: 100%;
	height: 100rpx;
	border-bottom: 2rpx solid #EFEFEF;
	background: #FFFFFF;
	margin-top: 20rpx;
	overflow: hidden;
}
.Order-details-D-img{
	width: 36rpx;
	height: 36rpx;
	margin-top: 30rpx;
	margin-left: 30rpx;
	float: left;
}
.Order-details-D-img>image{
	width: 100%;
	height: 100%;
}
.Order-details-D-zi{
	float: left; 
	height: 100%;
	width: 50%;
	line-height: 100rpx;
	font-size: 32rpx;
	color: #2E2E30;
	margin-left: 12rpx;
	font-weight: bold;
}
.Order-details-E{
	width: 100%;
	height: 250rpx;
	background: #FFFFFF;
	overflow: hidden;
	border-top: 2rpx solid #F5F5F5;
}
.Order-details-E-img{
	width: 168rpx;
	height: 174rpx;
	margin-left: 36rpx;
	margin-top: 24rpx;
	float: left;
}
.Order-details-E-img>image{
	width: 100%;
	height: 100%;
}
.Order-details-E-zi{
	width: 67%;
	height: 174rpx;
	margin-top: 24rpx;
	float: right;
	margin-right: 24rpx;
	position: relative;
}
.Order-details-E-zi>view:nth-of-type(1){
	font-size: 28rpx;
	color: #2E2E30;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.Order-details-E-zi>view:nth-of-type(2){
	font-size: 28rpx;
	color: #9B9B9B;
	line-height: 34rpx;
	display: -webkit-box;
	-webkit-line-clamp:2;
	overflow: hidden;
	text-overflow: ellipsis;
	-webkit-box-orient: vertical;
}
.Order-details-E-zi>view:nth-of-type(3){
	font-size: 28rpx;
	height: 30rpx;
	line-height: 30rpx;
	color: #9B9B9B;
	position: absolute;
	bottom: 0;
}
.Order-details-H{
	width: 100%;
	height: 170rpx;
	background: #FFFFFF;
	margin-top: 20rpx;
}
.Order-details-H-one{
	width: 90%;
	height: 30rpx;
	font-size: 28rpx;
	color: #2E2E30;
	padding-top: 30rpx;
	margin: 0 auto auto auto;
}
.Order-details-H-one>view:nth-of-type(1){
	float: left;
	line-height: 30rpx;
}
.Order-details-H-one>view:nth-of-type(2){
	float: right;
	line-height: 30rpx;
}
.Order-details-H-two{
	width: 90%;
	height: 30rpx;
	font-size: 28rpx;
	color: #2E2E30;
	padding-top: 20rpx;
	margin: 0 auto auto auto;
}
.Order-details-H-two>view:nth-of-type(1){
	line-height: 30rpx;
	float: left;
}
.Order-details-H-two>view:nth-of-type(2){
	float: right;
	line-height: 30rpx;
}
.Order-details-H-thr{
	width: 90%;
	height: 30rpx;
	font-size: 28rpx;
	color: #9B9B9B;
	padding-top: 10rpx;
	margin: 0 auto auto auto;
}
.Order-details-H-thr>view:nth-of-type(1){
	line-height: 30rpx;
	float: left;
}
.Order-details-H-thr>view:nth-of-type(2){
	float: right;
	line-height: 30rpx;
	color: #FF4D6A;
}
.Order-details-Z{
	width: 100%;
	height: 190rpx;
	background: #FFFFFF;
	position: fixed;
	bottom: 0;
}
.Order-details-Z-header{
	width: 100%;
	height: 90rpx;
	border-bottom: 2rpx solid #EFEFEF;
	border-top: 2rpx solid #EFEFEF;
}
.Order-details-Z-header>view:nth-of-type(1){
	line-height: 90rpx;
	margin-left: 36rpx;
	font-size: 14px;
	color: #2E2E30;
	float: left;
	font-weight: bold;
}
.Order-details-Z-header>view:nth-of-type(2){
	line-height: 90rpx;
	margin-right: 36rpx;
	font-size: 14px;
	color: #2E2E30;
	float: right;
	font-weight: bold;
}
.Order-details-Z-foot{
	width: 100%;
	height: 100rpx;
}
.Order-details-Z-one{
	width: 34%;
	height: 100%;
	font-size: 32rpx;
	color: #2E2E30;
	text-align: center;
	line-height: 100rpx;
	float: left;
}
.Order-details-Z-one>image{
	width: 32rpx;
	height: 32rpx;
	display: block;
	float: left;
	margin-left: 70rpx;
	margin-top: 34rpx;
}
.Order-details-Z-one>._span:nth-of-type(1){
	display: block;
	float: left;
	margin-left: 10rpx;
}
.Order-details-Z-one>._span:nth-of-type(2){
	color: #E2E2E2;
	display: block;
	float: right;
}
.Order-details-Z-two{
	width: 33%;
	height: 100%;
	font-size: 32rpx;
	color: #FF4D6A;
	text-align: center;
	line-height: 100rpx;
	float: left;
}
.Order-details-Z-thr{
	width: 33%;
	height: 100%;
	font-size: 32rpx;
	color: #FFFFFF;
	text-align: center;
	line-height: 100rpx;
	float: left;
	background-image: -webkit-linear-gradient(316deg, #15D49F 0%, #57D6CF 100%);
	background-image: linear-gradient(134deg, #15D49F 0%, #57D6CF 100%);
}
.Order-details-tian{
	width: 100%;
	height: 190rpx;
}

