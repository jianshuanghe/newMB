
.mybusiness-information{
	width: 100%;
	min-height: 50%;
	background: #FFFFFF;
	padding-bottom: 120rpx;
}
.top-s{
	margin-top: 40rpx;
}
.mybusiness-information-box{
	width: 90%;
	height: 300rpx;
	margin: 0 auto;
	border-bottom: 2rpx solid #F5F5F5;
}
.mybusiness-supply-con{
	width: 90%;
	min-height: 250rpx;
	margin: 0 auto;
	border-bottom: 2rpx solid #F5F5F5;
	overflow: hidden;
}
.mybusiness-supply-conone{
	width: 100%;
	min-height: 140rpx;
	margin-top: 40rpx;
}
.widtaaa{
	width: 100%!important;
}
.mybusiness-supply-conone-zi{
	width: 60%;
	height: 100%;
	float: left;
}
.mybusiness-supply-conone-zi>view:nth-of-type(1){
	width: 100%;
	height: 80rpx;
	font-size: 34rpx;
	color: #2E2E30;
	line-height: 40rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}
.mybusiness-supply-conone-zi>view:nth-of-type(2){
	width: 100%;
	height: 40rpx;
	line-height: 40rpx;
	margin-top: 16rpx;
}
.mybusiness-supply-conone-zi>view:nth-of-type(2)>._span:nth-of-type(1){
	border: 2rpx solid #3893FF;
	border-radius: 4rpx;
	width: 52rpx;
	height: 28rpx;
	font-size: 20rpx;
	color: #3893FF;
	text-align: center;
	line-height: 28rpx;
}
.mybusiness-supply-conone-zi>view:nth-of-type(2)>._span:nth-of-type(2){
	font-size: 22rpx;
	color: #9B9B9B;
	margin-left: 10rpx;
}
.mybusiness-supply-conone-zis{
	width: 100%;
	height: 100%;
	float: left;
}
.mybusiness-supply-conone-zis>view:nth-of-type(1){
	width: 100%;
	height: 80rpx;
	font-size: 34rpx;
	color: #2E2E30;
	line-height: 40rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}
.mybusiness-supply-conone-zis>view:nth-of-type(2){
	width: 100%;
	height: 40rpx;
	line-height: 40rpx;
	padding-left: 12rpx;
	margin-top: 40rpx;
}
.mybusiness-supply-conone-zis>view:nth-of-type(2)>._span:nth-of-type(1){
	border: 2rpx solid #3893FF;
	border-radius: 4rpx;
	padding: 6rpx 6rpx 6rpx 6rpx;
	font-size: 20rpx;
	color: #3893FF;
}
.mybusiness-supply-conone-zis>view:nth-of-type(2)>._span:nth-of-type(2){
	font-size: 22rpx;
	color: #9B9B9B;
	margin-left: 10rpx;
}
.mybusiness-supply-conone-tu{
	width: 226rpx;
	height: 166rpx;
	float: right;
	border-radius: 4rpx;
}
.mybusiness-supply-conone-tu>image{
	width: 100%;
	height: 100%;
}
.mybusiness-supply-bot{
	width: 100%;
	height: 120rpx;
	border-top: 2rpx solid #F5F5F5;
	position: fixed;
	bottom: 0;
	background: #FFFFFF;
}
.mybusiness-supply-Customer{
	width: 95%;
	height:90rpx;
	background: #02C2A2;
	margin: 15rpx auto;
	border-radius: 2px;
	font-size: 32rpx;
	color: #FFFFFF;
	text-align: center;
	line-height: 90rpx;
}
.myproduct-list-edit{
	width: 100%;
	height: 60rpx;
}
.product-book-BA{
	width: 13.5%;
	height: 26rpx;
	margin-top: 20rpx;
	line-height: 26rpx;
	font-size: 24rpx;
	color: #9B9B9B;
	border-right: 2rpx solid #E2E2E2;
	float: left;
	margin-left: 22rpx;
}
.right{
	border: 0;
}
.one{
	margin-left: 0;
}
.shou>image{
	width: 24rpx!important;
	height: 24rpx!important;
	margin-top: 0rpx !important;
}
.product-book-BA>image{
	width: 24rpx;
	height: 20rpx;
	margin-top: 2rpx;
	display: block;
	float: left;
}
.product-book-BA>._span{
	margin-left: 10rpx;
