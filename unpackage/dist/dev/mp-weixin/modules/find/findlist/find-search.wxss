
.find-search {
	width: 100%;
	min-height: 100%;
}
.quickBtn-search {
	width: 100vw;
	height: 11vw;
	padding: 0vw 3vw 0 3vw;
	background: #fff;
	border-bottom: 2rpx solid #f5f5f5;
}
.quickBtn-Fd {
	position: relative;
	width: 5%;
}
.search-Fd {
	position: relative;
}
.search-NI-img {
	position: relative;
	width: 60rpx;
}
.search-NI-img > image {
	width: 36rpx;
	height: 36rpx;
	margin-top: 22rpx;
	margin-left: 100rpx;
	z-index: 100;
}
.search-NI-text {
	position: relative;
	width: 440rpx;
	margin-left: 22rpx;
	margin-top: 10rpx;
	height: 60rpx;
	line-height: 60rpx;
	background: #f6f6f6;
	border-radius: 30rpx;
	padding-left: 68rpx;
	float: left;
}
.inputSearch {
	line-height: 60rpx;
	height: 60rpx;
	width: 98%;
	border: none;
	outline: none;
}
.search-NI-textp {
	position: relative;
	float: left;
	margin-top: 14rpx;
	font-size: 32rpx;
	color: #02c2a2;
	margin-left: 36rpx;
}
.find-search-LS {
	width: 100%;
	height: 93.5vh;
	background: #ffffff;
}
.find-search-LSP {
	font-size: 28rpx;
	color: #2e2e30;
	padding-left: 30rpx;
	padding-top: 48rpx;
}
.find-search-LSARR {
	width: 97%;
	min-height: 100rpx;
	margin: 0rpx auto;
}
.find-search-LSARR > view {
	padding: 10rpx 20rpx 10rpx 20rpx;
	background: #f5f5f5;
	float: left;
	font-size: 26rpx;
	color: #5d5d5d;
	margin-left: 20rpx;
	margin-top: 20rpx;
}
.clear {
	clear: both;
}
.find-search-bot {
	text-align: center;
	font-size: 24rpx;
	color: #5d5d5d;
	clear: both;
	position: fixed;
	bottom: 10rpx;
	width: 100%;
}

