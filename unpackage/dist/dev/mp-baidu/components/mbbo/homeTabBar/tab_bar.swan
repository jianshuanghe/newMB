<view class="tabBar-content"><view class="tabBarBox"><view class="tb-content"><view data-event-opts="{{[['tap',[['tabBarItems',[1]]]]]}}" class="left tb-box" bindtap="__e"><view class="tb-img"><image src="{{home}}"></image></view><view class="tb-text"><text class="{{[clickItems===1?'class-a':'']}}">{{i18n.home}}</text></view></view><view data-event-opts="{{[['tap',[['tabBarItems',[2]]]]]}}" class="left tb-box" bindtap="__e"><view class="tb-img"><image src="{{find}}"></image></view><view class="tb-text"><text class="{{[clickItems===2?'class-a':'']}}">{{i18n.find}}</text></view></view><view data-event-opts="{{[['tap',[['tabBarItems',[5]]]]]}}" class="left tb-box" bindtap="__e"><view class="tb-img-pub"><image src="{{published}}"></image></view></view><view data-event-opts="{{[['tap',[['tabBarItems',[3]]]]]}}" class="left tb-box" bindtap="__e"><view class="tb-img"><image src="{{news}}"></image></view><view class="tb-text"><text class="{{[clickItems===3?'class-a':'']}}">{{i18n.news}}</text></view><block s-if="{{$root.m0!==0&&List.noticeCount}}"><view class="noticeCount">{{List.noticeCount}}</view></block></view><view data-event-opts="{{[['tap',[['tabBarItems',[4]]]]]}}" class="left tb-box" bindtap="__e"><view class="tb-img"><image src="{{my}}"></image></view><view class="tb-text"><text class="{{[clickItems===4?'class-a':'']}}">{{i18n.my}}</text></view></view></view></view></view>